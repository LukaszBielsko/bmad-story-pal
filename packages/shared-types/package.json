{
  "name": "@storymagic/shared-types",
  "version": "1.0.0",
  "description": "Shared TypeScript type definitions for StoryMagic applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.ts'",
